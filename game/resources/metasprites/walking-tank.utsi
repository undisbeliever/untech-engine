  <frame id="turn_left_2" order="2">
    <gridlocation x="1" y="1"/>
    <tilehitbox x="4" y="7" width="22" height="18"/>
    <shield x="11" y="7" width="10" height="17"/>
    <hitbox x="11" y="8" width="10" height="16"/>
