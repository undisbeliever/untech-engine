    <hitbox x="7" y="8" width="19" height="16"/>
    <hurtbox x="14" y="7" width="13" height="18"/>
    <object size="large" x="4" y="3"/>
    <object size="small" x="20" y="3"/>
    <object size="small" x="20" y="11"/>
    <object size="small" x="4" y="19"/>
    <object size="small" x="20" y="19"/>
    <object size="small" x="12" y="19"/>
  </frame>
  <frame id="walk_left_2" order="2">
    <gridlocation x="2" y="0"/>
    <tilehitbox x="5" y="7" width="22" height="18"/>
    <shield x="6" y="7" width="7" height="18"/>
    <hitbox x="7" y="8" width="19" height="16"/>
    <hurtbox x="14" y="7" width="13" height="18"/>
    <object size="large" x="4" y="3"/>
    <object size="small" x="20" y="3"/>
    <object size="small" x="20" y="11"/>
    <object size="small" x="4" y="19"/>
    <object size="small" x="20" y="19"/>
    <object size="small" x="12" y="19"/>
  </frame>
  <frame id="walk_left_3" order="2">
    <gridlocation x="3" y="0"/>
    <tilehitbox x="5" y="7" width="22" height="18"/>
    <shield x="6" y="7" width="7" height="18"/>
    <hitbox x="7" y="8" width="19" height="16"/>
    <hurtbox x="14" y="7" width="13" height="18"/>
    <object size="large" x="4" y="3"/>
    <object size="small" x="20" y="3"/>
    <object size="small" x="20" y="11"/>
    <object size="small" x="4" y="19"/>
    <object size="small" x="20" y="19"/>
    <object size="small" x="12" y="19"/>
  </frame>
  <frame id="attack_left_a" order="2">
    <gridlocation x="4" y="0"/>
    <tilehitbox x="5" y="7" width="22" height="18"/>
    <shield x="6" y="7" width="7" height="18"/>
    <hitbox x="7" y="8" width="19" height="16"/>
    <hurtbox x="14" y="7" width="13" height="18"/>
    <object size="large" x="4" y="3"/>
    <object size="small" x="20" y="3"/>
