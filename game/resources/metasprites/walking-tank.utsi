  <frame id="turn_left_1" order="2">
    <gridlocation x="0" y="1"/>
    <tilehitbox x="5" y="7" width="22" height="18"/>
    <shield x="9" y="7" width="7" height="18"/>
    <hitbox x="10" y="8" width="12" height="16"/>
    <hurtbox x="17" y="7" width="6" height="18"/>
