    <object size="small" x="20" y="19"/>
    <object size="small" x="12" y="19"/>
  </frame>
  <animation id="walk_left" durationformat="DISTANCE_HORIZONTAL">
    <aframe frame="walk_left_1" duration="128"/>
    <aframe frame="attack_left_a" duration="128"/>
    <aframe frame="attack_left_b" duration="128"/>
    <aframe frame="walk_left_2" duration="192"/>
    <aframe frame="walk_left_2" duration="192"/>
    <aframe frame="walk_left_1" duration="128"/>
    <aframe frame="attack_left_a" duration="128"/>
    <aframe frame="attack_left_b" duration="128"/>
    <aframe frame="walk_left_3" duration="192"/>
    <aframe frame="walk_left_3" duration="192"/>
  </animation>
  <animation id="turn_left" durationformat="TIME" oneshot="true">
    <aframe frame="turn_left_1" hflip="true" duration="25"/>
    <aframe frame="turn_left_2" duration="25"/>
    <aframe frame="turn_left_1" duration="25"/>
  </animation>
  <animation id="defeated_left" durationformat="TIME" oneshot="true">
    <aframe frame="defeated_left" duration="25"/>
  </animation>
